.atlas_wrap{
    background: rgb(3, 17, 30);
    color: rgba(255,255,255,0.6);
    height: 100vh;
    width: 100vw;
}
.atlas_list{
    padding: 24rpx;
}
.img{
    background-repeat: no-repeat;
    background-position: center 80rpx;
    background-size: cover;
    width: 100%;
    height: 500rpx;
}
.img-title{
    color: #fff;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: flex-start;
}
.img-title-text{
    width: 100vw;
    text-align: center;
    margin-left: -80rpx;
    font-size: 34rpx;
}
.back{
    width: 80rpx;
    height: 100%;
    display: inline-flex;
    align-items: center;
    z-index: 999;
}
image {
    image-rendering: -moz-crisp-edges;
    image-rendering: -o-crisp-edges;
    image-rendering: -webkit-optimize-contrast;
    image-rendering: crisp-edges;
    -ms-interpolation-mode: nearest-neighbor;
}
.back-icon{
    width: 18rpx;
    height: 34rpx;
    margin-left: 34rpx;
}
